{
  "name": "@m-ld/m-ld",
  "version": "0.4.1-0",
  "description": "m-ld native Javascript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./dist/mqtt": "./dist/mqtt/index.js",
    "./dist/ably": "./dist/ably/index.js"
  },
  "scripts": {
    "link-locals": "npm link ../m-ld-spec ../json-rql ../typedoc-theme",
    "clean": "rm -rf dist _site",
    "build": "npm run clean && jest --silent && tsc && typedoc",
    "dev": "concurrently -r -k \"jest --watch --silent\" \"tsc --watch\"",
    "dev+log": "jest --watch",
    "compliance": "tsc --incremental && node compliance/test.js",
    "prepublishOnly": "npm run build && npm version $VERSION && git push",
    "postpublish": "npm dist-tag add \"@m-ld/m-ld@\"${npm_package_version} $(git branch --no-color --show-current)",
    "doc-dev": "concurrently \"onchange 'src/**/*.ts' 'doc/**/*.*' -- typedoc\" \"browser-sync start --server _site --files _site\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/m-ld/m-ld-js.git"
  },
  "author": "gsvarovsky@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/m-ld/m-ld-js/issues"
  },
  "homepage": "https://js.m-ld.org/",
  "devDependencies": {
    "@m-ld/typedoc-theme": "^0.1.4",
    "@types/jest": "^26.0.15",
    "@types/memdown": "^3.0.0",
    "@types/readable-stream": "^2.3.9",
    "aedes": "^0.44.0",
    "browser-sync": "^2.26.13",
    "concurrently": "^5.3.0",
    "jest": "^26.6.3",
    "jest-mock-extended": "^1.0.10",
    "leveldown": "^5.6.0",
    "memdown": "^5.1.0",
    "onchange": "^7.1.0",
    "restify": "^8.5.1",
    "tmp": "^0.2.1",
    "ts-jest": "^26.4.4",
    "typedoc": "^0.19.2",
    "typescript": "^4.1.2"
  },
  "jest": {
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/test/"
    ],
    "testMatch": [
      "<rootDir>/test/**/*.test.ts"
    ],
    "transform": {
      "^.+\\.(ts|tsx)?$": "ts-jest"
    }
  },
  "dependencies": {
    "@m-ld/m-ld-spec": "^0.5.0",
    "@rdfjs/data-model": "^1.2.0",
    "@types/abstract-leveldown": "^5.0.1",
    "@types/detect-node": "^2.0.0",
    "@types/jsonld": "^1.5.1",
    "@types/rdf-js": "^4.0.0",
    "ably": "^1.2.4",
    "async-mqtt": "^2.6.1",
    "detect-node": "^2.0.4",
    "json-rql": "^0.5.0",
    "jsonld": "^3.2.0",
    "local-storage": "^2.0.0",
    "loglevel": "^1.7.1",
    "marky": "^1.2.1",
    "mqtt-pattern": "^1.2.0",
    "quadstore": "^7.1.1",
    "rxjs": "^6.6.3",
    "short-uuid": "^4.1.0"
  }
}
