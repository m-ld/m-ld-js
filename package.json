{
  "name": "@m-ld/m-ld",
  "version": "0.9.0-edge.1",
  "description": "m-ld native Javascript",
  "main": "ext/index.js",
  "types": "ext/index.d.ts",
  "exports": {
    ".": "./ext/index.js",
    "./ext/orm": "./ext/orm/index.js",
    "./ext/mqtt": "./ext/mqtt/index.js",
    "./ext/socket.io": "./ext/socket.io/index.js",
    "./ext/socket.io-server": "./ext/socket.io/server/index.js",
    "./ext/ably": "./ext/ably/index.js",
    "./ext/wrtc": "./ext/wrtc/index.js",
    "./ext/security": "./ext/security/index.js",
    "./ext/shacl": "./ext/shacl/index.js",
    "./ext/constraints/WritePermitted": "./ext/constraints/WritePermitted.js",
    "./ext/constraints/Statutory": "./ext/constraints/Statutory.js"
  },
  "scripts": {
    "link-locals": "npm link ../m-ld-spec ../json-rql ../typedoc-theme",
    "clean": "rm -rf ext _site",
    "build": "npm run clean && jest --silent --runInBand && tsc && typedoc",
    "dev": "concurrently -r -k \"jest --watch --silent --runInBand\" \"tsc --watch\"",
    "dev+log": "jest --watch --runInBand",
    "compliance": "tsc --incremental && node compliance/test.js",
    "ver": "echo $npm_package_version",
    "doc-dev": "typedoc && concurrently \"onchange -d 1000 'src/**/*.ts' 'doc/**/*.*' 'typedoc.js' -- typedoc\" \"browser-sync start --server _site --files _site\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/m-ld/m-ld-js.git"
  },
  "author": "gsvarovsky@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/m-ld/m-ld-js/issues"
  },
  "homepage": "https://js.m-ld.org/",
  "devDependencies": {
    "@m-ld/io-js-build": "^0.1.0",
    "@m-ld/typedoc-theme": "^0.1.4",
    "@peculiar/webcrypto": "^1.2.3",
    "@types/jest": "^29.0.1",
    "@types/n3": "^1.4.4",
    "@types/readable-stream": "^2.3.9",
    "@types/ws": "^8.5.3",
    "ably": "^1.2.10",
    "aedes": "^0.48.0",
    "async-mqtt": "^2.6.1",
    "browser-sync": "^2.26.14",
    "classic-level": "^1.2.0",
    "concurrently": "^7.2.2",
    "generic-pool": "^3.8.2",
    "jest": "^29.0.3",
    "jest-mock-extended": "^2.0.6",
    "memory-level": "^1.0.0",
    "n3": "^1.8.0",
    "onchange": "^7.1.0",
    "restify": "^8.5.1",
    "simple-peer": "^9.11.0",
    "socket.io": "^4.1.2",
    "socket.io-client": "^4.1.2",
    "tape": "^5.3.1",
    "tmp": "^0.2.1",
    "ts-jest": "^29.0.0",
    "typedoc": "^0.23.19",
    "typedoc-plugin-no-inherit": "1.2.2",
    "typescript": "^4.1.3"
  },
  "jest": {
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/test/"
    ],
    "testMatch": [
      "<rootDir>/test/**/*.test.ts"
    ],
    "transform": {
      "^.+\\.(ts|tsx)?$": "ts-jest"
    }
  },
  "dependencies": {
    "@ably/msgpack-js": "^0.4.0",
    "@m-ld/jsonld": "^6.0.1-m-ld.0",
    "@m-ld/m-ld-spec": "^0.6.7",
    "@types/detect-node": "^2.0.0",
    "@types/requestidlecallback": "^0.3.1",
    "@types/sha.js": "^2.4.0",
    "@types/simple-peer": "^9.11.4",
    "@types/valid-data-url": "^2.0.0",
    "abstract-level": "^1.0.3",
    "asynciterator": "^3.2.0",
    "cuid": "^2.1.8",
    "json-rql": "^0.6.2",
    "loglevel": "^1.7.1",
    "marky": "^1.2.1",
    "mqtt-pattern": "^1.2.0",
    "quadstore": "^11.0.3",
    "quadstore-comunica": "^3.0.3",
    "rdf-data-factory": "^1.0.4",
    "reflect-metadata": "^0.1.13",
    "rx-flowable": "^0.1.0",
    "rxjs": "^7.2.0",
    "sha.js": "^2.4.11",
    "sparqlalgebrajs": "^4.0.3",
    "valid-data-url": "^4.0.0"
  },
  "peerDependencies": {
    "@peculiar/webcrypto": "1",
    "ably": "1",
    "async-mqtt": "2",
    "simple-peer": "9",
    "socket.io": "4",
    "socket.io-client": "4"
  },
  "peerDependenciesMeta": {
    "socket.io": {
      "optional": true
    },
    "socket.io-client": {
      "optional": true
    },
    "async-mqtt": {
      "optional": true
    },
    "ably": {
      "optional": true
    },
    "simple-peer": {
      "optional": true
    },
    "@peculiar/webcrypto": {
      "optional": true
    }
  }
}
